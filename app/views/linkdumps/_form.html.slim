= form_for(@linkdump) do |f|
  - if @linkdump.errors.any?
    #error_explanation
      h2
        = pluralize(@linkdump.errors.count, "error")
        |  prohibited this linkdump from being saved:
      ul
        - @linkdump.errors.full_messages.each do |message|
          li
            = message
  .field
    = f.label :url
    br
    = f.text_field :url
  .actions
    = f.submit
